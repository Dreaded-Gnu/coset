{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": false
  },
  "lib": [
    "node",
    "es6",
    "dom"
  ],
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "mode": "modules",
    "out": "doc",
    "exclude": "**/node_modules/**",
    "theme": "default",
    "ignoreCompilerErrors": false,
    "excludePrivate": false,
    "excludeNotExported": false,
    "target": "ES6",
    "moduleResolution": "node",
    "preserveConstEnums": "true",
    "stripInternal": false,
    "suppressExcessPropertyErrors": false,
    "suppressImplicitAnyIndexErrors": false,
    "module": "commonjs",
    "readme": "README.md",
    "external-modulemap": ".*packages\/(@coset\/[^\/]+)\/.*",
    "verbose": true,
    "name": "@coset"
  }
}